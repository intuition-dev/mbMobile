{
   "name": "ElBuild",
   "productName": "ElBuild",
   "version": "0.0.1",
   "main": "app/index.js",
   "homepage": "./",
   "scripts": {
      "start": "electron .",
      "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=res/icon.icns --prune=true --out=release-builds",
      "package-win": "electron-packager . MyApp --overwrite --asar=true --platform=win32 --arch=ia32 --icon=res/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ElBuild\""
   },
   "dependencies": {
      "mbake": "^4.10.11"
   },
   "devDependencies": {
      "electron-packager": ">=12.1.2",
      "electron": "^3.0.4",
      "electron-builder": ">=20.28.4"
   }
}
